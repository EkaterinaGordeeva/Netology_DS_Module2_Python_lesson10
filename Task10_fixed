   "source": [
    "#Задание 9\n",
    "#матрица переменной\n",
    "def cov(x,y):\n",
    "    return np.sum((x-x.mean()*y-y.mean())/len(x)-1)\n",
    "#матрица ковариаций\n",
    "def cov_mat(X):\n",
    "    return np.array([[cov(X[0],X[0]),cov(X[0],X[1])],[cov(X[1],X[0]),cov(X[1],X[1])]])\n",
    "\n",
    "X=np.vstack((data_norm[0],data_norm[1]))\n",
    "cov_mat(X.T)"
   ]
  },
