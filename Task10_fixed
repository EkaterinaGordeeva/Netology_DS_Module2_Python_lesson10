    "#Задание 4\n",
    "\n",
    "#Визуально угол между 2п/3 и 3п/4\n",
    "import math\n",
    "theta = 130*3.14/180\n",
    "#Задаю матрицу синусов,косинусов\n",
    "u=np.matrix('{};{}'.format(math.sin(theta),math.cos(theta)))\n",
    "yp=Y*u\n",
    "var_emperical=np.var(yp)\n",
    "var_theoretical=u.T*C*u\n",
    "print(var_emperical,var_theoretical)\n"
   ]
